        "name": "detr-resnet-50-panoptic-huggingface-deploy",
        "versions": {
          "dtlpy": "1.89.14"
        },
        "secrets": [],
        "runtime": {
          "podType": "gpu-t4",
          "concurrency": 1,
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 1000
          },
          "preemptible": false
        }
      }
